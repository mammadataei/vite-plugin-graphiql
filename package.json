{
  "name": "vite-plugin-graphiql",
  "version": "1.1.0",
  "description": "Integrate GraphiQL IDE in your Vite projects",
  "author": "Mohammad Ataei",
  "license": "MIT",
  "keywords": [
    "vite",
    "vite-plugin",
    "graphiql",
    "graphql"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mammadataei/vite-plugin-graphiql/"
  },
  "bugs": {
    "url": "https://github.com/mammadataei/vite-plugin-graphiql/issues"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && pnpm build:node && pnpm build:client",
    "build:node": "unbuild",
    "build:client": "vite build",
    "dev:client": "vite",
    "preview:client": "vite preview",
    "example": "pnpm --filter example",
    "format": "prettier --write . && pnpm lint --fix",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --fix",
    "*.{js,jsx,ts,tsx,md,html,css}": "prettier --write"
  },
  "dependencies": {
    "pathe": "^1.1.2",
    "sirv": "^2.0.4"
  },
  "peerDependencies": {
    "vite": "^2.9.0 || ^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@graphiql/plugin-explorer": "^0.1.22",
    "@graphiql/toolkit": "^0.8.4",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^2.2.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.10.0",
    "graphiql": "^2.4.7",
    "graphql": "^16.8.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "prettier": "2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.0",
    "unbuild": "^1.2.1",
    "vite": "^3.2.7"
  },
  "packageManager": "pnpm@8.13.1"
}
